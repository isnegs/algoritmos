algoritmo "LEE01A"
var
   a, b, c, d, p, s, aux : inteiro
inicio
      escreva("Valor A: ")
      leia(a)
      escreva("Valor B: ")
      leia(b)
      escreva("Valor C: ")
      leia(c)
      escreva("Valor D: ")
      leia(d)
      
      p <- a * c
      s <- b + d
      
      escreval("Resultado do produto:",p)
      escreval("Resultado da soma:",s)

fimalgoritmo
