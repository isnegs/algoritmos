algoritmo "LE01H"
var
   volume, comprimento, largura, altura : real
inicio
      escreva("Comprimento da caixa: ")
      leia(comprimento)
      escreva("Largura da caixa: ")
      leia(largura)
      escreva("Altura da caixa: ")
      leia(altura)
      
      volume <- comprimento * largura * altura
      
      escreval("Volume da caixa:",volume)
fimalgoritmo
